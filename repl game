puts "You are now entering Super Mario World!"
puts "Please select your character:"
puts "Mario or Luigi"
character = gets.chomp.downcase

until character == "mario" || character == "luigi"
  puts "You can only be Mario or Luigi"
  puts "Please select your character again!"
  puts "Mario or Luigi"
  character = gets.chomp.downcase
end

case character
when "mario"
  puts "
  It's a me, Mario!
  "
  puts "
